{
  "expo": {
    "name": "Productive+",
    "slug": "rn-productive-plus",
    "owner": "amir_gendargenoevskiy",
    "version": "1.2.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#293559",
          "sounds": [
            "./assets/note.wav"
          ]
        }
      ]
    ],
    "platforms": [
      "ios",
      "android"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/iOS_icon.png",
      "supportsTablet": true,
      "userInterfaceStyle": "light",
      "bundleIdentifier": "com.amirkerimov.productive-plus",
      "buildNumber": "1.2.1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#293559"
      },
      "userInterfaceStyle": "dark",
      "package": "com.amirkerimov.productive_plus",
      "versionCode": 4,
      "permissions": ["WRITE_EXTERNAL_STORAGE", "RECEIVE_BOOT_COMPLETED"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
